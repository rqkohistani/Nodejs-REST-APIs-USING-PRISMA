{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto",
        "singleQuote": true,
        "printWidth": 120
      }
    ],
    "camelcase": [
      "error",
      {
        "ignoreImports": true
      }
    ],
    "no-shadow": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": "warn"
  },
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2021
  }
}